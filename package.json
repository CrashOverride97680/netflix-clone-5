{
  "name": "netflix-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-responsive-carousel": "^3.1.25",
    "react-scripts": "1.0.13",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build-css": "node-sass src/main.scss -o src/",
    "watch-css":
      "yarn run build-css && node-sass src/main.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "run-p watch-css start-js",
    "build": "yarn run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "bootstrap-sass": "^3.3.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "font-awesome": "^4.7.0",
    "jest-css": "^1.0.1",
    "jest-file": "^1.0.0",
    "node-sass": "^4.5.3",
    "prettier-eslint": "^8.1.1",
    "react-test-renderer": "^15.6.1"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": ["react-app"]
  },
  "license": "MIT"
}
